    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.3",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "swagger-ui-express": "^5.0.1",
    "swagger2openapi": "^7.0.8",
    "winston": "^3.17.0"
