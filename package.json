    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "passport": "^0.7.0",
    "express-async-errors": "^3.1.1",
    "express-list-endpoints": "^7.1.1",
    "express-oas-generator": "^1.0.46",
    "express-session": "^1.18.1",
    "generate-schema": "^2.6.0",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jet-schema": "^1.4.2",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "micromatch": "^4.0.8",
